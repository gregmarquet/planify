<ul class="list-group">
  <h3>Incomplete</h3>
  <% @entries.incomplete.each do |entry| %>
    <li class="list-group-item"><%= entry.title %></li>
  <% end %>
  <h3>Completed</h3>
  <% @entries.completed.each do |entry| %>
    <li class="list-group-item"><%= entry.title %></li>
  <% end %>
  <h3>Abandoned</h3>
  <% @entries.abandoned.each do |entry| %>
    <li class="list-group-item"><%= entry.title %></li>
  <% end %> 
</ul>
